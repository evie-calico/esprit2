{
    "runtime": {
        "version": "LuaJIT",
        "path": ["meta/?.lua"],
        "pathStrict": true
    },
    "format.defaultConfig": {
        "indent_style": "tab"
    },
    "diagnostics.groupFileStatus": {
        "ambiguity": "Any",
        "await": "Any",
        "codestyle": "Any",
        "duplicate": "Any",
        "global": "Any",
        "luadoc": "Any",
        "redefined": "Any",
        "strict": "Any",
        "strong": "Any",
        "type-check": "Any",
        "unbalanced": "Any",
        "unused": "Any"
    },
    "diagnostics.neededFileStatus": {
        "spell-check": "None!"
    }
}
